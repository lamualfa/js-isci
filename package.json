{
  "name": "js-isci",
  "version": "1.0.4",
  "main": "mutable.js",
  "license": "MIT",
  "dependencies": {
    "date-format": "^3.0.0",
    "rfdc": "^1.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babelify": "^10.0.0",
    "benchmark": "^2.1.4",
    "browserify": "^16.5.0",
    "codecov": "^3.6.1",
    "jest": "^24.9.0",
    "uglify-es": "^3.3.9"
  },
  "description": "ISCI library for Javascript",
  "repository": "https://github.com/laodemalfatih/js-isci",
  "author": "Laode Muhammad Al Fatih <laodemalfatih@gmail.com>",
  "scripts": {
    "test": "jest && codecov",
    "build:mutable:js": "browserify mutable.js -t [ babelify --presets [ @babel/preset-env ] ] --standalone isci | uglifyjs -c -m --source-map -o dist/mutable.min.js",
    "build:immutable:js": "browserify immutable.js -t [ babelify --presets [ @babel/preset-env ] ] --standalone isci | uglifyjs -c -m --source-map -o dist/immutable.min.js",
    "build:mutable:es": "browserify mutable.js --standalone isci | uglifyjs -c -m --source-map -o dist/mutable.es.min.js",
    "build:immutable:es": "browserify immutable.js --standalone isci | uglifyjs -c -m --source-map -o dist/immutable.es.min.js",
    "build": "yarn build:mutable:js&&yarn build:immutable:js&&yarn build:mutable:es&&yarn build:immutable:es"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "keywords": [
    "isci",
    "id",
    "unique-id",
    "sequence-id",
    "increment",
    "identification",
    "scheme",
    "id-generator",
    "random"
  ]
}
